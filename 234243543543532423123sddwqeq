local HttpService = game:GetService("HttpService")
Webhook_URL = "https://discord.com/api/webhooks/1162813576352894997/JCdvQLjASGJBkWIodz5sbV-e2z5Vq2BAqo6VLoPyN07YBwVvunRcrhu49UfqK72afiGY"
local S_hwid = game:GetService("RbxAnalyticsService"):GetClientId()
local plre = game.Players.LocalPlayer
local WV = "nil."
local DName = game.Players.LocalPlayer.DisplayName
local UName = game.Players.LocalPlayer.Name

local WhitelistedHwids = {
-- Whenever people execute this script it will tell u the hwid
"EA3B1811-0F8B-4B47-B533-0F783BD66D92" -- my hwid for example
}

local BlacklistedHwids = {

}

if not table.find(WhitelistedHwids, S_hwid) and not table.find(BlacklistedHwids, S_hwid) then
WV = "No"
else
WV = "Yes"
end


if table.find(BlacklistedHwids, S_hwid) then
WV = "Blacklisted"
end

local response =
    request(
    {
        Url = Webhook_URL,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode(
            {
                ["content"] = "",
                ["embeds"] = {
                    {
                        ["title"] = "Style has been executed.",
                        ["description"] = DName.."/"..UName.." has executed the script. Whitelisted? "..WV.. ".",
                        ["type"] = "rich",
                        ["color"] = embedcolor,
                        ["fields"] = {
                            {
                                ["name"] = "Hardware ID:",
                                ["value"] = S_hwid,
                                ["inline"] = true
                            }
                        }
                    }
                }
            }
        )
    }
)

if not table.find(WhitelistedHwids, S_hwid) and not table.find(BlacklistedHwids, S_hwid) then
plre:Kick("You are not whitelisted, "..DName..".")
while true do 
local part = Instance.new("Part",workspace)
part.Name = "FUCKYOU"
end
    else
    print("HWID Passed")
end


if table.find(BlacklistedHwids, S_hwid) then
plre:Kick("Due to you trying to use my script, you are now blacklisted "..DName.. ". ~doma")
while true do 
local part = Instance.new("Part",workspace)
part.Name = "FUCKYOU"
    end
end
