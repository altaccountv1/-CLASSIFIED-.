if table.find(allowed, S_hwid) then
  _G.WV = "Yes"
else
  _G.WV = "No"
end

local embedcolor

if _G.WV == "Yes" then
  _G.embedcolor = "4388219"
else
  _G.embedcolor = "14887209"
end

local response =
    request(
    {
        Url = Webhook_URL,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode(
            {
                ["content"] = "",
                ["embeds"] = {
                    {
                        ["title"] = "Dragon Style has been executed.",
                        ["description"] = DName.. " / "..UName.. " has executed Dragon style. \n \n **Whitelist Value** \n".._G.WV.. "\n **UserId** \n "..tostring(plre.UserId),
                        ["type"] = "rich",
                        ["color"] = _G.embedcolor,
                        ["fields"] = {
                           {
                        ["name"] = "Hardware Id",
                        ["value"] = S_hwid,
                        ["inline"] = true
                           }
                        }
                    }
                }
            }
        )
    }
)

if _G.WV == "No" then
  return plre:Kick("You are not whitelisted")
end
